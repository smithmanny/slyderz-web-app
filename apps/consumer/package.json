{
  "name": "slyderz-consumer",
  "version": "1.0.0",
  "engines": {
    "node": ">=18.18.0",
    "pnpm": ">=8"
  },
  "scripts": {
    "dev": "next dev",
    "build": "NODE_ENV=production next build && pnpm run migrate",
    "start": "next start",
    "studio": "pnpm exec drizzle-kit studio --port 8000",
    "migrate": "pnpm exec drizzle-kit generate:pg && pnpm run migrate:apply",
    "migrate:apply": "pnpm exec tsx ./drizzle/migrate.ts",
    "migrate:rollback": "pnpm exec drizzle-kit drop",
    "lint": "pnpm exec @biomejs/biome check --apply ./app",
    "test": "jest",
    "test:watch": "jest --watch",
    "postinstall": "pnpm run migrate"
  },
  "prisma": {
    "schema": "db/schema.prisma"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "pnpm run lint"
    ]
  },
  "dependencies": {
