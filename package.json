{
  "name": "blitz-slyderz",
  "version": "1.0.0",
  "scripts": {
    "dev": "blitz dev",
    "build": "blitz build",
    "start": "blitz start",
    "studio": "blitz prisma studio",
    "lint": "eslint --ignore-path .gitignore --ext .js,.ts,.tsx .",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "prisma": {
    "schema": "db/schema.prisma"
  },
  "prettier": {
    "semi": false,
    "printWidth": 100
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && pretty-quick --staged",
      "pre-push": "tsc && npm run lint && npm run test"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@date-io/core": "1.3.13",
    "@date-io/date-fns": "1.3.13",
    "@material-ui/core": "4.12.3",
    "@material-ui/icons": "4.11.2",
    "@material-ui/pickers": "3.3.10",
    "@prisma/client": "~2.30.3",
    "@reduxjs/toolkit": "1.6.1",
    "@sendgrid/mail": "^7.4.7",
    "@stripe/react-stripe-js": "1.4.1",
    "@stripe/stripe-js": "1.17.1",
    "blitz": "0.39.0",
    "classnames": "2.3.1",
    "date-fns": "2.23.0",
    "final-form": "4.20.2",
    "mdi-material-ui": "6.22.1",
    "mui-rff": "3.0.13",
    "prisma": "~2.30.3",
    "prop-types": "15.7.2",
    "randomstring": "^1.2.1",
    "react": "alpha",
    "react-dom": "alpha",
    "react-final-form": "6.5.3",
    "react-redux": "7.2.4",
    "stripe": "8.174.0",
    "typescript": "4.4.2",
    "uuid": "8.3.2",
    "zod": "3.8.2"
  },
  "devDependencies": {
    "@types/preview-email": "2.0.1",
    "@types/react": "17.0.19",
    "eslint": "7.32.0",
    "husky": "7.0.2",
    "lint-staged": "11.1.2",
    "prettier": "2.3.2",
    "prettier-plugin-prisma": "2.30.0",
    "pretty-quick": "3.1.1",
    "preview-email": "3.0.5"
  },
  "private": true
}
