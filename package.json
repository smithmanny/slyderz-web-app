{
  "name": "blitz-slyderz",
  "version": "1.0.0",
  "scripts": {
    "dev": "blitz dev",
    "build": "blitz build",
    "start": "blitz start",
    "studio": "blitz prisma studio",
    "lint": "eslint --ignore-path .gitignore --ext .js,.ts,.tsx .",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "prisma": {
    "schema": "db/schema.prisma"
  },
  "prettier": {
    "semi": false,
    "printWidth": 100
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && pretty-quick --staged",
      "pre-push": "tsc && npm run lint && npm run test"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@date-io/core": "2.13.1",
    "@date-io/date-fns": "2.13.1",
    "@emotion/react": "^11.7.0",
    "@emotion/styled": "^11.6.0",
    "@mui/icons-material": "^5.2.1",
    "@mui/material": "^5.2.3",
    "@mui/styles": "^5.2.3",
    "@mui/x-date-pickers": "^5.0.0-beta.0",
    "@prisma/client": "^4.0.0",
    "@reduxjs/toolkit": "1.8.0",
    "@sendgrid/mail": "^7.6.0",
    "@stripe/react-stripe-js": "1.7.0",
    "@stripe/stripe-js": "1.24.0",
    "blitz": "0.45.3",
    "classnames": "2.3.1",
    "date-fns": "2.28.0",
    "final-form": "4.20.6",
    "mdi-material-ui": "7.2.0",
    "mui-rff": "5.0.0",
    "notistack": "^2.0.5",
    "prisma": "^4.0.0",
    "prop-types": "15.8.1",
    "randomstring": "^1.2.1",
    "react": "0.0.0-experimental-6a589ad71",
    "react-dom": "0.0.0-experimental-6a589ad71",
    "react-final-form": "6.5.8",
    "react-redux": "7.2.6",
    "stripe": "8.207.0",
    "typescript": "4.6.2",
    "uuid": "8.3.2",
    "zod": "3.13.4"
  },
  "devDependencies": {
    "@types/preview-email": "2.0.1",
    "@types/react": "17.0.39",
    "eslint": "8.10.0",
    "husky": "7.0.4",
    "lint-staged": "12.3.4",
    "prettier": "2.5.1",
    "prettier-plugin-prisma": "3.10.0",
    "pretty-quick": "3.1.3",
    "preview-email": "3.0.5"
  },
  "private": true
}
