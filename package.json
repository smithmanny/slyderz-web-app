    "@mui/material": "^5.2.3",
    "@mui/styles": "^5.2.3",
    "@prisma/client": "~3.6.0",
    "@reduxjs/toolkit": "1.7.0",
    "@sendgrid/mail": "^7.6.0",
    "@stripe/react-stripe-js": "1.6.0",
    "@stripe/stripe-js": "1.22.0",
    "blitz": "0.44.4",
    "classnames": "2.3.1",
    "date-fns": "2.27.0",
    "final-form": "4.20.6",
    "mdi-material-ui": "7.1.0",
